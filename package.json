{
  "name": "gatsby-cookie-notice",
  "description": "Gatsby Plugin that add a Cookie Component fully customizable working with gatsby-plugin-gdpr-cookies.",
  "author": "Florent Vandroy",
  "keywords": ["gatsby", "cookie", "GDPR", "notice", "customizable"],
  "version": "1.0.3",
  "private": false,
  "main": "dist/index.js",
  "module": "dist/index.js",
  "files": [ "dist", "README.md" ],
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "5.0.0",
    "web-vitals": "^2.1.2"
